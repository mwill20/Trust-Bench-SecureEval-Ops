# Copy this file to `.env` and populate the secrets before running Trust_Bench.

# Optional: default provider-specific API keys.
OPENAI_API_KEY=
GROQ_API_KEY=
GEMINI_API_KEY=

# Control runtime behaviour.
ENABLE_SECURITY_FILTERS=true
TRUST_BENCH_WORKDIR=./output

# Optional overrides for the web interface (fallback to defaults when omitted).
LLM_PROVIDER=openai
OPENAI_MODEL=gpt-4o-mini
GROQ_MODEL=llama-3.1-8b-instant
GEMINI_MODEL=gemini-1.5-flash

